{
    "redirects": [
        {
            "source": "/(.*)",
            "destination": "https://www.revobloom.in/$1",
            "permanent": true
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=3600"
                },
                {
                    "key": "Accept-Ranges",
                    "value": "none"
                }
            ]
        }
    ]
}